{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": false,
    "member-access": false,
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "no-console": false,
    "no-empty": false,
    "curly": false,
    "variable-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
